<!DOCTYPE html>
<html lang="th">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />

    <title><%= (typeof title != "undefined" ? title : "No Title") %></title>
  </head>

  <body>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <div class="container-fluid">
        <a class="navbar-brand" href="/">MVC[1]</a>       
        </div>
      </div>
    </nav>

    <div>
      <div>
        <a href="https://google.com">โจทย์</a>
      </div>
        <div id="table" class="container">
            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">ID</th>
                    <th scope="col">NAME</th>
                    <th scope="col">LATITUDE</th>

                  </tr>
                </thead>
                <tbody>
                    <% if (response) { %> <% response.forEach(function(a){ %>
                        <tr>
                            <td ><%= a.student_id %></td>
                            <td ><%= a.name %></td>
                            <td ><%= a.last_name %></td>

                                  </tr>
                          <% }) %> <% } %>
                </tbody>
              </table>
        </div>
    </div>

    <footer class="bg-light text-center text-lg-start">
      <div class="text-center p-3" style="background-color: rgba(0, 0, 0, 0.2)">
        © 2022 Made By:
        <a class="text-dark" href="https://github.com/zergreen/"
          >63050159 Phongsakorn Kawhaolai</a
        >
      </div>
    </footer>
  </body>
</html>
